<!DOCTYPE html>
<html lang="<%= locals.language || 'en' %>">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= locals.title %>
    </title>

    <link rel="icon" type="image/png" href="/favicon.ico" />

    <!-- PrimÃ¦re Meta Tags -->
    <title>
        <%= locals.title %>
    </title>
    <meta name="title" content="<%= locals.title %>">
    <meta name="description" content="<%= locals.description %>">

    <!-- Open Graph / Facebook MD -->
    <meta property="og:type" content="website">
    <meta property="og:url" content="<%= locals.url %>">
    <meta property="og:title" content="<%= locals.title %>">
    <meta property="og:description" content="<%= locals.description %>">
    <meta property="og:image" content="<%= locals.url %>/favicon.ico">

    <!-- Twitter MD -->
    <meta property="twitter:card" content="summary_large_image">
    <meta property="twitter:url" content="<%= locals.url %>">
    <meta property="twitter:title" content="<%= locals.title %>">
    <meta property="twitter:description" content="<%= locals.description %>">
    <meta property="twitter:image" content="<%= locals.url %>/favicon.ico">

    <!-- CSS -->
    <link rel="stylesheet" href="css/app.css">

    <!-- Socket.io -->
    <script src="socket.io.js"></script>

    <!-- JavaScript -->
    <script defer src="hotreload.js"></script>
    <script defer src="js/app.js"></script>
</head>

<!-- Kroppen bliver fyldt automatisk af bundle.js -->

<body>

    <div class="svelte-component" data-name="boilerplate" data-props="{}"></div>

</body>

</html>