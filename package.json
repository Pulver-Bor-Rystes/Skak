{
  "name": "tstest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.1",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "rollup": "^2.46.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "sass": "^1.32.12",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.7.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "start": "yarn tsc --project config/tsconfig.json; node compiled_server/main.js",
    "server": "yarn nodemon compiled_server/main.js --ignore 'web/*'",
    "server_compiler": "yarn tsc -w --project config/tsconfig.json",
    "svelte_compiler": "yarn rollup -c config/rollup.config.js -w",
    "style_compiler": "yarn sass web/svelte/*.*ss web/compiled_public/style.css -w",
    "pretty": "yarn prettier --write server/ web/svelte --config config/.prettierrc.json",
    "compile_all": "node compile.js",
    "clean": "rm -rf compiled_server; rm -rf web/compiled_public"
  },
